from github import Github

# Создание экземпляра класса Github с использованием персонального токена
g = Github('персональный_токен')

# Получение репозитория по имени пользователя и имени репозитория
repo = g.get_user().get_repo('имя_репозитория')

# Создание нового файла в репозитории
file = repo.create_file('/путь/к/файлу', 'Новый файл', 'Содержимое файла', branch='основная_ветвь')
